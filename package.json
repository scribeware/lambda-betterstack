{
  "name": "lambda-betterstack",
  "version": "1.0.0",
  "description": "A Lambda function to forward CloudWatch logs to Betterstack/Logtail",
  "main": "handler.js",
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "eslint --ext .js .",
    "format": "prettier --write \"**/*.js\"",
    "deploy:dev": "serverless deploy --aws-profile serverless-dev --stage dev",
    "deploy:qa": "serverless deploy --aws-profile serverless-dev --stage qa",
    "deploy:prod": "serverless deploy --stage prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "author": "David Furnes <dfurnes@dosomething.org>",
  "license": "MIT",
  "devDependencies": {
    "@dosomething/eslint-config": "^5.0.1",
    "eslint": "^7.30.0",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^2.7.0",
    "jest": "^27.0.1",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "serverless": "^3.40.0"
  },
  "dependencies": {
    "@logtail/node": "^0.5.5",
    "@logtail/winston": "^0.5.5",
    "winston": "^3.17.0",
    "zlib": "^1.0.5"
  },
  "packageManager": "yarn@4.9.2+sha512.1fc009bc09d13cfd0e19efa44cbfc2b9cf6ca61482725eb35bbc5e257e093ebf4130db6dfe15d604ff4b79efd8e1e8e99b25fa7d0a6197c9f9826358d4d65c3c"
}
